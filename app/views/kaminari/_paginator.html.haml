= paginator.render do
  %nav.d-flex.justify-content-center.my-6
    %ul.pagination
      = prev_page_tag unless current_page.first?
      - each_page do |page|
        - if page.display_tag?
          = page_tag page
      - if !current_page.out_of_range? && !current_page.last?
        = next_page_tag
